{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:window:allow-set-title",
    "core:default",
    "opener:default",
    "dialog:default",
    "os:default",
    "os:allow-hostname",
    "store:default",
    "notification:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*.sndtest.com"
        },
        {
          "url": "http://*.baidu.com"
        },
        {
          "url": "http://127.0.0.1:8000"
        },
        {
          "url": "https://*.api.nvidia.com"
        }
      ],
      "deny": [
        {
          "url": "http://private.tauri.app"
        }
      ]
    }
  ]
}